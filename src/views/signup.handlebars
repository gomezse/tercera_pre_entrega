<form action="/api/sessions/signup" method="post">
    First Name: <input type="text" name="first_name" id="first_name">
    <br>
    Last Name: <input type="text" name="last_name" id="last_name">
    <br>
    Email: <input type="email" name="email" id="email">
    <br>
    Password: <input type="password" name="password" id="password">
    <br>
    <input type="submit" value="Ingresar">
</form>

<button onclick="location.href='/api/sessions/auth/github'">GITHUB</button>
<button onclick="location.href='/api/sessions/auth/google'">GOOGLE</button>